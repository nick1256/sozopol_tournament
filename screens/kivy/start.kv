<StartScreen>:

    BoxLayout:
		padding: 20
		spacing: 10
		orientation : "vertical"
		Button:
			on_press: root.go_to_create_tournament()
			background_color: [1,0,0,1]
			text: "Create Tournament"
		Button:
			background_color: [0,0,1,1]
			text: "Load Tournament"
			on_press: root.load_tournament()

