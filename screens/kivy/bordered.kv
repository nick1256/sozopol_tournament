<Bordered>:
    canvas:
        Color:
            rgba: 0, 1, 0, .3
        Line:
            width: 1.
            rectangle: (self.x, self.y, self.width, self.height)

<ColoredLabel>:
	canvas:
		Color:
			rgba: 0,0,1,.3
		Line:
            width: 3.
            rectangle: (self.x, self.y, self.width, self.height)
